{
  "name": "simple-nest-app",
  "version": "0.2.0",
  "description": "",
  "author": {
    "email": "anthony.williams.cs@gmail.com",
    "name": "Anthony Williams",
    "url": "https://github.com/Wolven531/simple-nest-app"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist && npm i && npm run test",
    "start": "node dist/main",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start",
    "start:dev:watch": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --coverage --coveragePathIgnorePatterns=.+\\.module\\.ts$ --coveragePathIgnorePatterns=.+\\.dto\\.ts$ --coveragePathIgnorePatterns=.+\\.model\\.ts$ --coveragePathIgnorePatterns=^index\\.ts$ --coveragePathIgnorePatterns=^main\\.ts$",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:no-cov": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/testing": "^7.6.15",
    "class-transformer": "^0.4.0",
    "compression": "^1.7.4",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "swagger-ui-themes": "^3.0.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
